<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  android:layout_width="fill_parent"android:layout_height="fill_parent"android:orientation="vertical"android:background="#ffffff"android:gravity="left"><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  android:layout_width="fill_parent"android:layout_height="fill_parent"android:orientation="horizontal"android:background="#ffffff"android:gravity="left"><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_id"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="5dp"	android:paddingTop="5dp" 	android:paddingBottom="5dp" 	android:textStyle="bold"    android:textSize="18sp"    android:textColor="#574A22"     android:gravity="left"/><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_tr_name"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="5dp"	android:paddingTop="5dp" 	android:paddingBottom="5dp" 	android:textStyle="bold"    android:textSize="18sp"    android:textColor="#574A22"     android:gravity="left" /><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/_id"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="5dp"	android:paddingTop="5dp" 	android:paddingBottom="5dp" 	android:textStyle="bold"    android:textSize="18sp"    android:textColor="#ffffff"    	android:gravity="left" /></LinearLayout><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"  android:layout_width="fill_parent"android:layout_height="fill_parent"android:orientation="horizontal"android:background="#ffffff"android:gravity="left"><TextView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="0dp"	android:paddingTop="0dp" 	android:paddingBottom="5dp" 	android:textStyle="bold"    android:textSize="16sp"    android:textColor="#574A22"     android:text="#"    android:gravity="left" /><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_order"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="0dp" 	android:paddingRight="5dp"	android:paddingTop="0dp" 	android:paddingBottom="5dp" 	android:textStyle="normal"    android:textSize="16sp"    android:textColor="#574A22"     android:gravity="left" /><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_en_name"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="0dp" 	android:paddingRight="5dp"	android:paddingTop="0dp" 	android:paddingBottom="5dp" 	android:textStyle="normal"    android:textSize="16sp"    android:textColor="#574A22"     android:gravity="left" /><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_type"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="5dp"	android:paddingTop="0dp" 	android:paddingBottom="5dp" 	android:textStyle="normal"    android:textSize="16sp"    android:textColor="#574A22"     android:gravity="left" /><TextView xmlns:android="http://schemas.android.com/apk/res/android"	android:id="@+id/chapter_verse_count"    android:layout_width="wrap_content"    android:layout_height="wrap_content"	android:paddingLeft="5dp" 	android:paddingRight="5dp"	android:paddingTop="0dp" 	android:paddingBottom="5dp" 	android:textStyle="normal"    android:textSize="16sp"    android:textColor="#574A22"     android:gravity="left" /></LinearLayout></LinearLayout>